<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/single_dp" android:color="@color/sig_color_base_blue_regular_any" />
            <solid android:color="@color/sig_color_base_blue_regular_any" />
            <corners android:radius="@dimen/legacy_scrollbar_radius" />
        </shape>
    </item>
    <item android:left="@dimen/legacy_scrollbar_radius">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/single_dp" android:color="@color/sig_color_base_blue_regular_any" />
            <solid android:color="@color/sig_color_base_blue_regular_any" />
            <corners android:radius="@dimen/default_gap_quarter" />
        </shape>
    </item>
</layer-list>