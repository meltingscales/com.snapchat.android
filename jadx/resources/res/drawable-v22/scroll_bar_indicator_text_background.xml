        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/scroll_bar_indicator_color"/>
            <solid android:color="@color/scroll_bar_indicator_color"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
</layer-list>
