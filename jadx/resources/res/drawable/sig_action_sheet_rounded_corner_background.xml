<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="?attr/sigColorBackgroundSurface"/>
            <corners android:radius="@dimen/action_sheet_corner_radius"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="?attr/sigColorBackgroundSurfaceDown"/>
            <corners android:radius="@dimen/action_sheet_corner_radius"/>
        </shape>
    </item>
</selector>
