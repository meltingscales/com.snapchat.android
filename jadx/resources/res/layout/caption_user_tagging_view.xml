<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/caption_carousel_container" android:layout_width="match_parent" android:layout_height="@dimen/caption_carousel_recycler_view_height">
    <com.snap.previewtools.shared.view.CarouselRecyclerView android:id="@+id/style_carousel_recycler_view" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="@dimen/caption_carousel_recycler_view_height"/>
    <com.snap.previewtools.shared.view.CarouselRecyclerView android:id="@+id/tagging_carousel_recycler_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/caption_carousel_recycler_view_height"/>
    <ImageButton android:layout_gravity="start|center_vertical" android:id="@+id/caption_floating_tag_button_new" android:background="@drawable/floating_tag_button_new" android:visibility="gone" android:layout_width="@dimen/caption_floating_tag_button_size" android:layout_height="@dimen/caption_floating_tag_button_size"/>
    <ImageView android:layout_gravity="start|bottom" android:id="@+id/caption_selector" android:visibility="gone" android:layout_width="@dimen/caption_carousel_selector_width" android:layout_height="@dimen/caption_carousel_selector_height" android:layout_marginBottom="@dimen/caption_carousel_selector_margin_bottom" android:src="@drawable/svg_captions_selector_chevron" android:layout_marginStart="@dimen/caption_carousel_selector_margin_start"/>
</FrameLayout>
